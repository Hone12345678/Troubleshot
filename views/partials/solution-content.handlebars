{{#each this}}
<div id="{{id}}"" class="shadow-inner px-6 rounded bg-slate-700 border border-slate-500">
  <h2 class="text-center underline text-zinc-300 text-2xl">{{capitalize_first name}}</h2>
  <span class="text-center text-zinc-300">Priority: {{priority}}</span>
  <pre class="text-center flex-wrap text-slate-200 text-base">Solution: {{solution}}</pre>
</div>
{{/each}} 